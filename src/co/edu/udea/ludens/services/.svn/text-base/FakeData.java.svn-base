	public static void addUser(String login,String name,String passw, String role){
		userId++;
		User user = null;
		user = new User();

		user.setId(userId+"");
		user.setLogin(login);
		user.setName(name);
		user.setPassword(passw);
		user.setRole(role);

		users.put(user.getLogin(), user);
		
	}

